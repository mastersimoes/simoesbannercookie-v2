<!-- Modal -->
<div class="modal fade" id="bannercookie" tabindex="-1" data-backdrop="static" data-keyboard="false" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h1 class="modal-title {$ColorTitle} " id="exampleModalLabel"> {$MessageTitle} </h1>
            </div>
            <div class="modal-body">
                <h5 class=" {$ColorText} "> {$MessageText} </h5>
            </div>
            <div class="modal-footer">
                <button type="button" id="aceptar" class="btn {$ColorAcceptButton} "> {$ButtonTextAccept} </button>
            </div>
        </div>
    </div>
</div>